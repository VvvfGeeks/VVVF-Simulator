    <LinearGradientBrush x:Key="TitleBarBrush" StartPoint="0,0" EndPoint="0,1">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{DynamicResource KeyColor}" Offset="0"/>
                <GradientStop Color="{DynamicResource KeyDarkColor}" Offset="1"/>
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="TitleBarTextBrush" Color="{DynamicResource KeyTextColor}"/>
